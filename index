<body>
<div id="map" style="width: 800px;height: 500px"></div>
<script src='http://code.jquery.com/jquery-2.1.0.min.js'></script>
<script src="http://cdn.leafletjs.com/leaflet-0.7.3/leaflet.js"></script>
<script src="file:///C:/Users/user/Desktop/New folder (3)/Leaflet.awesome-markers-2.0-develop/dist/leaflet.awesome-markers.js"></script>
<script>
var map = L.map('map').setView([4.6, 2.9], 5);
attr = 
'<a href="http://openstreetmap.org">openstreetmap</a>,'+'<a href="http://mapsnigeriainitiative.wordpress.com">mapsNI</a>';
L.tileLayer('http://{s}.tiles.wmflabs.org.com/hikebike/{z}/{x}/{y}.png', {
	maxZoom: 19,
	attribution: '&copy;' + attr,
}).addTo(map);

L.marker([9.00, 7.00], {icon: L.AwesomeMarkers.icon({icon: 'glass', prefix: 'fa', markerColor: 'green', spin:true}) }).addTo(map);

var icon = L.AwesomeMarkers.icon({
icon: 'coffee',
markerColor: 'red',
prefix: 'fa'
});

$.getJSON('Cities.geojson', function(data){
L.geoJson(data, {
pointToLayer: function(feature, latlng){
marker = L.marker(latlng, {icon: icon})
marker.bindPopup(feature.properties.address);
return marker; 
}}).addTo(map);
});

</script>
